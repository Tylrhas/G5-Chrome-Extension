{
  "manifest_version": 2,

  "name": " G5 Hub Place of Intrest",
  "description": " Future home of Bookmarklets for the G5 WCS Team",
  "version": "0.1",
  "background": {
    "scripts": [
      "content_scripts.js",
      "jquery-3.2.1.min.js"
    ],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "icon.png",
    //"default_popup": "popup.html"
    "default_title": "Place!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://maps.googleapis.com/"
  ]

}