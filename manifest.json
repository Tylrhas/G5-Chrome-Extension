{
  "manifest_version": 2,

  "name": "WCS Functions",
  "description": "Future home of Bookmarklets for the G5 WCS Team",
  "version": "1.0",

  "icons": {
    "48": "icon.png"
  },

  "permissions": [
    "tabs", "<all_urls>","storage"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
